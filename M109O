#include <bits/stdc++.h>
using namespace std;
using int64 = long long;
const int64 MOD = 1000000009LL;
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int N;
    if (!(cin >> N)) return 0;
    if (N == 0) { cout << 1 << '\n'; return 0; }
    if (N == 1) { cout << 2 << '\n'; return 0; }
    int64 f_prev2 = 1;
    int64 f_prev1 = 2; 
    for (int k = 2; k <= N; ++k) {
        __int128 a = f_prev1;
        __int128 b = f_prev2;
        int64 a2 = (int64)((a * a) % MOD);
        int64 b2 = (int64)((b * b) % MOD);
        int64 b4 = (int64)(( (__int128)b2 * b2 ) % MOD);
        int64 term = (1 + a2) % MOD;
        term = (term + (int64)(( (__int128)b2 * a ) % MOD)) % MOD;
        term = (term - b4) % MOD;
        if (term < 0) term += MOD;
        f_prev2 = f_prev1;
        f_prev1 = term;
    }
    cout << f_prev1 % MOD << '\n';
    return 0;
}
